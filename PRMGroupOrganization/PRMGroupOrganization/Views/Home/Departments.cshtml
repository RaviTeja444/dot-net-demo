@model List<PRMGroupOrganization.Models.Department>
@{
    ViewBag.Title = "Departments";
}

<h2>Departments</h2>
<table class="table table-bordered">
    <thead>
        <tr>
            <th>DeptId</th>
            <th>DeptName</th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Model.Count; i++)
        {
            <tr>
                <td>@Model[i].DepartmentID</td>
                <td>@Model[i].DepartmentName</td>
            </tr>

        }
    </tbody>
</table>
      

